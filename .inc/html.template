<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$>
<head>
  <meta charset="utf-8">
  <!-- rumex Version -->
  <meta name="generator" content="$meta_generator$">
  <!-- rumex Suchmaschine -->
  <meta name="robots" content="$meta_robots$">
$for(author-meta)$
  <meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$">
$endif$
  <title>$if(title-prefix)$$title-prefix$ - $endif$$pagetitle$</title>
  <style type="text/css">code{white-space: pre;}</style>
  <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
$if(quotes)$
  <style type="text/css">q { quotes: "“" "”" "‘" "’"; }</style>
$endif$
$if(highlighting-css)$
  <style type="text/css">
$highlighting-css$
  </style>
$endif$
$for(css)$
  <link rel="stylesheet" href="$css$">
$endfor$
$if(math)$
  $math$
$endif$
  <!-- rumex RSS -->
  <link rel="alternate" type="application/rss+xml" title="$rsstitel$" href="$rssfile$" />
$for(header-includes)$
  $header-includes$
$endfor$
</head>
<body>
$for(include-before)$
$include-before$
$endfor$
$if(title)$
<header>
<h1 class="title"><a title="zur Startseite" href="index.html">$title$</a></h1>
$for(author)$
<h2 class="author"><a title="zur Startseite" href="index.html">$author$</a></h2>
$endfor$
$if(date)$
<h3 class="date"><a title="zur Startseite" href="index.html">$date$</a></h3>
$endif$
</header>
$endif$
$if(toc)$
<nav id="$idprefix$TOC">
$toc$
</nav>
$endif$
<div id=seite>
$body$
</div>
$for(include-after)$
$include-after$
$endfor$
</body>
</html>
